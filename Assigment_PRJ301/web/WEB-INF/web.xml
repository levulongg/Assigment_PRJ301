<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ListStudent</servlet-name>
        <servlet-class>Controller.DisplayGroup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Attendance</servlet-name>
        <servlet-class>Controller.Check</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TimeTable</servlet-name>
        <servlet-class>Controller.Schedule</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckAttend</servlet-name>
        <servlet-class>Controller.CheckAttend</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GroupStudent</servlet-name>
        <servlet-class>Controller.GroupStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeachingSchedule</servlet-name>
        <servlet-class>Controller.TeachingSchedule</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateCheckattend</servlet-name>
        <servlet-class>Controller.UpdateAttend</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Timetable</servlet-name>
        <servlet-class>Controller.Timetable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckAttendend</servlet-name>
        <servlet-class>Controller.CheckAttendend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckAttend</servlet-name>
        <url-pattern>/CheckAttend</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GroupStudent</servlet-name>
        <url-pattern>/GroupStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeachingSchedule</servlet-name>
        <url-pattern>/TeachingSchedule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateCheckattend</servlet-name>
        <url-pattern>/UpdateCheckattend</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Timetable</servlet-name>
        <url-pattern>/Timetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckAttendend</servlet-name>
        <url-pattern>/CheckAttendend</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
